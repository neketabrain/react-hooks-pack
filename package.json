{
  "name": "react-hooks-pack",
  "version": "0.3.3",
  "description": "Custom React hooks package",
  "publishConfig": {
    "access": "public"
  },
  "author": "Nikita Karapuzov <karapuzov.nikita@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neketabrain/react-hooks-pack"
  },
  "homepage": "https://github.com/neketabrain/react-hooks-pack",
  "bugs": "https://github.com/neketabrain/react-hooks-pack/issues",
  "keywords": [
    "react",
    "reactjs",
    "hooks",
    "react-hooks",
    "utils",
    "tools",
    "helpers"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "browser": "dist/index.bundle.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "rollup -cw",
    "build": "npm run clean && rollup -c",
    "lint": "eslint --cache src/**/*.{ts,tsx}",
    "lint:fix": "npm run lint --fix",
    "test": "jest --maxWorkers 2"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.2.4",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^10.0.9",
    "path": "^0.12.7",
    "prettier": "^2.0.2",
    "raf-stub": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.2.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-jest": "^25.3.0",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && lint-staged",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}

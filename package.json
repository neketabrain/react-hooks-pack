{
  "name": "react-hooks-pack",
  "version": "0.1.0",
  "description": "React hooks collection",
  "author": "Nikita Karapuzov <karapuzov.nikita@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neketabrain/react-hooks-pack"
  },
  "homepage": "https://github.com/neketabrain/react-hooks-pack",
  "bugs": "https://github.com/neketabrain/react-hooks-pack/issues",
  "keywords": [
    "react",
    "hooks"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist/",
    "README.md"
  ],
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "deploy": "npm run build && npm publish",
    "lint": "eslint --cache src/**/*.{js,ts,jsx,tsx}"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.9",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    ".{js,ts,jsx.tsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
